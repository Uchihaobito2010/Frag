{
  "functions": {
    "api/check.py": {
      "maxDuration": 15,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/check",
      "dest": "/api/check.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/api/check.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/check.py"
    }
  ]
}
